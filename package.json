{
  "name": "legal-aider",
  "version": "0.1.0",
  "description": "A system for generating and analyzing legal AI interactions",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/main.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "generate-chat": "ts-node src/scripts/generateChat.ts",
    "archive-chats": "ts-node src/scripts/archiveChats.ts",
    "analyze-chats": "ts-node src/scripts/analyzeAndUpdateChats.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@langchain/anthropic": "^0.3.19",
    "@langchain/core": "^0.3.49",
    "dotenv": "^16.0.3",
    "langchain": "^0.3.24",
    "openai": "^4.96.0",
    "react-email": "^4.0.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^22.15.0",
    "@types/uuid": "^9.0.8",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "yargs": "^17.7.2"
  }
}
